<div class="card">
    <div class="card-image">
        <img src="{{urlToImage}}" alt="">
        <h3 class="card-title">{{title}}</h3>
    </div>
    <div class="card-content">
        <p class="card-description">{{description}}</p>
    </div>
    <div class="card-action">
        <a href="{{url}}">Read more</a>
    </div>
</div>